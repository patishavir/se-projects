SELECT
  STR_ITROT_KLALI_FCT.MCH,
  replace (ODS_MCH_DMN.SHEM_MALE_HEB,'"',' '),
  STR_SNIF_DMN.SNIF,
  STR_SNIF_DMN.TEUR_SNIF_AROCH_HEB,
  Sum(NVL(STR_ITROT_KLALI_FCT.ITRA_KLALIT,0)) / 
COUNT(DISTINCT STR_TAARICH_DMN.TR ),
  Sum(NVL(STR_ITROT_KLALI_FCT.ITRA_KLALIT_MT,0)) / 
COUNT(DISTINCT STR_TAARICH_DMN.TR ),
  STR_TAARICH_DMN.TR,
  STR_MUTZAR_KLALI_DMN.SCH,
  STR_MUTZAR_KLALI_DMN.TEUR_SCH_AROCH,
  STR_MUTZAR_KLALI_DMN.TEUR_NOSE_RASHI,
  STR_MUTZAR_KLALI_DMN.KOD_NOSE_RASHI,
  STR_MUTZAR_KLALI_DMN.TEUR_NOSE,
  STR_MUTZAR_KLALI_DMN.KOD_NOSE,
  STR_ITROT_KLALI_FCT.MIS_ZIHUY_MUTZAR,
  STR_MATBEOT_DMN.TEUR_MATBEA,
  STR_MATBEOT_DMN.KOD_MATBEA,
  STR_SNIF_DMN.BANK,
  STR_SNIF_DMN.TEUR_BANK
FROM
  ODS_MCH_DMN,
  STR_SNIF_DMN,
  STR_ITROT_KLALI_FCT,
  STR_TAARICH_DMN,
  STR_MUTZAR_KLALI_DMN,
  STR_MATBEOT_DMN,
  STR_CHESBON_LAKOACH_LKP
WHERE
  ( ODS_MCH_DMN.GK_SNIF=STR_SNIF_DMN.GK_SNIF and ODS_MCH_DMN.BANK=STR_SNIF_DMN.BANK  )
  AND  ( STR_TAARICH_DMN.TR=STR_ITROT_KLALI_FCT.TR  )
  AND  ( STR_ITROT_KLALI_FCT.BANK=STR_MATBEOT_DMN.BANK and STR_ITROT_KLALI_FCT.KOD_MATBEA=STR_MATBEOT_DMN.KOD_MATBEA  )
  AND  ( STR_ITROT_KLALI_FCT.GK_MCH=ODS_MCH_DMN.GK_MCH  )
  AND  ( STR_MUTZAR_KLALI_DMN.GK_MUTZAR=STR_ITROT_KLALI_FCT.GK_MUTZAR  )
  AND  ( STR_TAARICH_DMN.SW_KLL=1  )
  AND  ( STR_CHESBON_LAKOACH_LKP.KOD=ODS_MCH_DMN.KOD_CHESHBON_LAKOACH  )
  AND  ( STR_MUTZAR_KLALI_DMN.KOD_NOSE_KD<>2  )
  AND  (
      STR_TAARICH_DMN.TR = '26-may-2013'
  AND  DECODE(STR_TAARICH_DMN.SW_YOM_ASAKIM_SHL,0,'לא',1,'כן','לא ידוע')  =  'כן'
  AND  STR_MUTZAR_KLALI_DMN.SCH  NOT IN  (627)
  AND  STR_CHESBON_LAKOACH_LKP.KOD  =  1
  )
GROUP BY
  STR_ITROT_KLALI_FCT.MCH, 
  replace (ODS_MCH_DMN.SHEM_MALE_HEB,'"',' '), 
  STR_SNIF_DMN.SNIF, 
  STR_SNIF_DMN.TEUR_SNIF_AROCH_HEB, 
  STR_TAARICH_DMN.TR, 
  STR_MUTZAR_KLALI_DMN.SCH, 
  STR_MUTZAR_KLALI_DMN.TEUR_SCH_AROCH, 
  STR_MUTZAR_KLALI_DMN.TEUR_NOSE_RASHI, 
  STR_MUTZAR_KLALI_DMN.KOD_NOSE_RASHI, 
  STR_MUTZAR_KLALI_DMN.TEUR_NOSE, 
  STR_MUTZAR_KLALI_DMN.KOD_NOSE, 
  STR_ITROT_KLALI_FCT.MIS_ZIHUY_MUTZAR, 
  STR_MATBEOT_DMN.TEUR_MATBEA, 
  STR_MATBEOT_DMN.KOD_MATBEA, 
  STR_SNIF_DMN.BANK, 
  STR_SNIF_DMN.TEUR_BANK
